<Window x:Class="ToolkitM9.Flash"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Fastboot Flash..." Height="165" Width="340" Icon="Resources/info_orange.ico">
    <Grid>
        <ComboBox x:Name="cbPartition" HorizontalAlignment="Left" Margin="62,10,0,0" VerticalAlignment="Top" Width="120" Height="24"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Partition:" VerticalAlignment="Top" Height="24"/>
        <Button x:Name="btnBrwsFile" Content="Browse" HorizontalAlignment="Left" Margin="247,68,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="62,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="260" IsReadOnly="True"/>
        <TextBlock HorizontalAlignment="Left" Margin="35,40,0,0" TextWrapping="Wrap" Text="File:" VerticalAlignment="Top" Width="22" Height="23"/>
        <Button x:Name="btnGoFlash" Content="Flash" HorizontalAlignment="Left" Margin="239,100,0,0" VerticalAlignment="Top" Width="83" Height="24"/>

    </Grid>
</Window>
